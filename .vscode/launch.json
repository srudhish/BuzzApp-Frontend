{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug Android",
      "type": "reactnative",
      "request": "launch",
      "platform": "android",
      "cwd": "${workspaceFolder}",
      "program": "${workspaceFolder}\\index.js",
      "console": "integratedTerminal",
      "sourceMaps": true
    },
    {
      "name": "Attach to Packager",
      "type": "reactnative",
      "request": "attach",
      "cwd": "${workspaceFolder}",
      "port": 8081,
      "sourceMaps": true
    }
  ]
}